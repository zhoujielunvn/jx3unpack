--战斗轻功技能替换
function SettFlySkillSkill(player)
	if tFlySkill[player.dwForceID] then
		if player.dwForceID ~= 0 then
			if player.bBirdMove == true and not player.IsHaveBuff(13735, 1)then
				RemoteCallToClient(player.dwID, "OnSkillReplace", tFlySkill[player.dwForceID].UpSkillID, tFlySkill[player.dwForceID].DownSkillID, 1, tFlySkill[player.dwForceID].UpSkillID)
				local lv = player.GetSkillLevel(9003)
				if lv ~= 0 then
					RemoteCallToClient(player.dwID, "OnSkillReplace", 9003, 20788, lv, 9003)
				end
				if player.dwForceID == 3 then
					local lv1 = player.GetSkillLevel(427)
					if player.GetSkillLevel(5669) ~= 1 then
						RemoteCallToClient(player.dwID, "OnActionBarSkillReplace", 427, 21134, lv1)
					end
				end
			else
				RemoteCallToClient(player.dwID, "OnSkillReplace", tFlySkill[player.dwForceID].DownSkillID, tFlySkill[player.dwForceID].UpSkillID, 1, tFlySkill[player.dwForceID].DownSkillID)
				local lv = player.GetSkillLevel(9003)
				if lv ~= 0 then
					RemoteCallToClient(player.dwID, "OnSkillReplace", 20788, 9003, lv, 20788)
				end
				if player.dwForceID == 3 then
					local lv2 = player.GetSkillLevel(427)
					if player.GetSkillLevel(5669) ~= 1 then
						RemoteCallToClient(player.dwID, "OnActionBarSkillReplace", 21134, 427, lv2)
					end
				end
			end
		end
	end
end

--魂阵重新激活
function SettlanternSkill(player)
	if not player then
		return
	end

	local scene = player.GetScene()
	if not scene then
		return
	end

	local hundeng1
	local hundeng2
	local hundeng3
	local buff1 = player.GetBuff(17743, 1)
	if buff1 then
		hundeng1 = GetNpc(buff1.nCustomValue)
	end
	local buff2 = player.GetBuff(17744, 1)
	if buff2 then
		hundeng2 = GetNpc(buff2.nCustomValue)
	end
	local buff3 = player.GetBuff(17745, 1)
	if buff3 then
		hundeng3 = GetNpc(buff3.nCustomValue)
	end

	if hundeng1 and hundeng2 and hundeng3 then
		local Distance1 = GetCharacterDistance(hundeng1.dwID, hundeng2.dwID)
		local Distance2 = GetCharacterDistance(hundeng2.dwID, hundeng3.dwID)
		local Distance3 = GetCharacterDistance(hundeng3.dwID, hundeng1.dwID)
		local Height1 = (hundeng1.nZ - hundeng2.nZ) / 64 / 8
		local Height2 = (hundeng2.nZ - hundeng3.nZ) / 64 / 8
		local Height3 = (hundeng3.nZ - hundeng1.nZ) / 64 / 8
		--需求高度差在10尺以内
		if Distance1 < 60 * 64 and Distance2 < 60 * 64 and Distance3 < 60 * 64 and Height1 <= 10 and Height2 <= 10 and Height3 <= 10 then
			local npctable = {hundeng1.dwID, hundeng2.dwID, hundeng3.dwID}
			local nConvexIndex = scene.CalcConvexHull(3, npctable)
			local Npchz = scene.CreatePet(100123, player.nX, player.nY, player.nZ, 0, 150 * 16, player.dwID)

			if Npchz then
				player.AddBuff(player.dwID, player.nLevel, 18231, 1) --阵法存在标记
				ProcessPlayerSkillNpc(Npchz, player) --NPC无敌+势力关系+浮空需求
				Npchz.dwConvexHullID = nConvexIndex
				player.CastSkill(25806, 1)  --局连线表现
			end
		else
			player.CastSkill(25807, 1) --局去掉连线表现
		end
	end
end
--用于刀宗添加破绽
function AddFlaw(player, target)
	if not player then
		return
	end
	if not target then
		return
	end

	local nType
	if IsPlayer(target.dwID) then
		nType = TARGET.PLAYER
	else
		nType = TARGET.NPC
	end

	--给目标添加的破绽Buff
	local nCount --破绽时长
	if player.GetSkillLevel(32499) == 1 then
		nCount = 120
	else
		nCount = 60
	end

	local buff_stack = target.GetBuffByOwner(24056, 1, player.dwID)
	if player.GetSkillLevel(32500) == 1 then
		target.AddBuff(player.dwID, player.nLevel, 24056, 1, nCount)
	else
		if not buff_stack then
			target.AddBuff(player.dwID, player.nLevel, 24056, 1, nCount)
		elseif buff_stack.nStackNum <= 3 then
			target.AddBuff(player.dwID, player.nLevel, 24056, 1, nCount)
		else
			target.SetBuffLeftActiveCount(buff_stack.nIndex, nCount)
			target.SetBuffNextActiveFrame(buff_stack.nIndex, 1 + GetLogicFrameCount())
		end
	end
	local buff_stack = target.GetBuffByOwner(24056, 1, player.dwID)
	if not buff_stack then
		return
	end

	--目标映射给刀宗的监控Buff
	local buff_jk = player.GetBuffByOwner(23955, 1, target.dwID)
	if buff_jk then
		for i = 1, buff_jk.nStackNum do
			player.DelBuffByIndex(buff_jk.nIndex)
		end
		player.AddBuff(target.dwID, target.nLevel, 23955, 1, nCount, 1, buff_stack.nStackNum)
	else
		player.AddBuff(target.dwID, target.nLevel, 23955, 1, nCount, 1, buff_stack.nStackNum)
	end

	--目标选择框表现Buff
	target.AddBuff(player.dwID, player.nLevel, 24059, buff_stack.nStackNum, nCount)
	if player.GetSkillLevel(32500) == 1 then
		player.CastSkill(32621, math.min(buff_stack.nStackNum + 4, 10), nType, target.dwID) --指定降低目标伤害
	else
		player.CastSkill(32621, math.min(buff_stack.nStackNum, 4), nType, target.dwID) --指定降低目标伤害 --指定降低目标伤害
	end
end

--空中后撤技能(特殊处理多姿态的藏剑、霸刀)
function SetRetreatSkill(player)
	if tRetreatData then
		local buff1 = player.GetBuff(tRetreatData[player.dwForceID][1].OriginalBuffID, 1)
		local buff2 = player.GetBuff(tRetreatData[player.dwForceID][2].SwitchBuffID, 1)
		if buff1 then
			if player.dwForceID == 8 then
				if player.GetKungfuMountID() == 10144 then
					player.CastSkill(20422, 9)
				elseif player.GetKungfuMountID() == 10145 then
					player.CastSkill(20422, 11)
				end
			elseif player.dwForceID == 23 then
				if player.IsHaveBuff(10814, 1) then
					player.CastSkill(20422, 19)
				elseif player.IsHaveBuff(10815, 1) then
					player.CastSkill(20422, 21)
				else
					player.CastSkill(20422, 23)
				end
			else
				player.CastSkill(tRetreatData[player.dwForceID][1].OriginalSkillID, (tRetreatData[player.dwForceID][1].OriginalSkillLv))
			end
		elseif buff2 then
			if player.dwForceID == 8 then
