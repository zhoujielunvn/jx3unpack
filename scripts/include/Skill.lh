--霸刀取风墙终点
function GetDestinationPoint(nX, nY, nDir, nLength)
	local Dex_X
	local Dex_Y
	local Dir_x = nDir
	local Dir_y = nDir

	if nDir > 64 and nDir < 128 then
		Dir_x = nDir
		Dir_y = 128 - nDir
	elseif nDir >= 128 and nDir < 192 then
		Dir_x = 256 - nDir
		Dir_y = 128 - nDir
	elseif nDir <= 256 and nDir >= 192 then
		--nDir = nDir - 256
		Dir_x = 256 - nDir
		Dir_y = nDir - 256
	end

	Dex_X = nX + math.cos(math.rad(Dir_x * 360 / 256)) * nLength
	Dex_Y = nY + math.sin(math.rad(Dir_y * 360 / 256)) * nLength

	return Dex_X, Dex_Y
end

--霸刀取横版风墙起点和终点
function GetSourceAndDestinationPoint(nX, nY, nDir, nLength)
	local Sou_X
	local Sou_Y
	local Dex_X
	local Dex_Y
	local Sou_dir = nDir + 60
	local Des_dir = nDir - 60
	local Sou_Dir_x
	local Sou_Dir_y
	local Des_Dir_x
	local Des_Dir_y

	if Sou_dir > 256 then
		Sou_dir = Sou_dir - 256
	end

	if Des_dir < 0 then
		Des_dir = Des_dir + 256
	end

	if Sou_dir > 64 and Sou_dir < 128 then
		Sou_Dir_x = Sou_dir
		Sou_Dir_y = 128 - Sou_dir
	elseif Sou_dir >= 128 and Sou_dir < 192 then
		Sou_Dir_x = 256 - Sou_dir
		Sou_Dir_y = 128 - Sou_dir
	elseif Sou_dir <= 256 and Sou_dir >= 192 then
		--nDir = nDir - 256
		Sou_Dir_x = 256 - Sou_dir
		Sou_Dir_y = Sou_dir - 256
	else
		Sou_Dir_x = Sou_dir
		Sou_Dir_y = Sou_dir
	end

	if Des_dir > 64 and Des_dir < 128 then
		Des_Dir_x = Des_dir
		Des_Dir_y = 128 - Des_dir
	elseif Des_dir >= 128 and Des_dir < 192 then
		Des_Dir_x = 256 - Des_dir
		Des_Dir_y = 128 - Des_dir
	elseif Des_dir <= 256 and Des_dir >= 192 then
		--nDir = nDir - 256
		Des_Dir_x = 256 - Des_dir
		Des_Dir_y = Des_dir - 256
	else
		Des_Dir_x = Des_dir
		Des_Dir_y = Des_dir
	end

	Sou_X = nX + math.cos(math.rad(Sou_Dir_x * 360 / 256)) * nLength
	Sou_Y = nY + math.sin(math.rad(Sou_Dir_y * 360 / 256)) * nLength

	Dex_X = nX + math.cos(math.rad(Des_Dir_x * 360 / 256)) * nLength
	Dex_Y = nY + math.sin(math.rad(Des_Dir_y * 360 / 256)) * nLength

	return Sou_X, Sou_Y, Dex_X, Dex_Y
end

--根据2点取朝向
function PiontToDir(player, sou_x, sou_y, dex_x, dex_y)
	local nFace = 0
	local nRnd = 0

	if sou_x < dex_x and sou_y < dex_y then
		nRnd = math.atan((dex_y - sou_y) / (dex_x - sou_x ))
	elseif sou_x > dex_x and sou_y < dex_y then
		nRnd = math.atan((dex_y - sou_y) / (dex_x - sou_x )) + math.pi
	elseif sou_x > dex_x and sou_y > dex_y then
		nRnd = math.atan((dex_y - sou_y) / (dex_x - sou_x )) + math.pi
	elseif sou_x < dex_x and sou_y > dex_y then
		nRnd = math.atan((dex_y - sou_y) / (dex_x - sou_x )) + math.pi * 2
	end

	if sou_y == dex_y then
		if sou_x > dex_x then
			nRnd = math.pi
		elseif sou_x < dex_x then
			nRnd = 0
		end
	end

	if sou_x == dex_x then
		if sou_y > dex_y then
			nRnd = math.pi * 3 / 2
		elseif sou_y < dex_y then
			nRnd = math.pi / 2
		end
	end

	if sou_x == dex_x and sou_y == dex_y then
		return player.nFaceDirection
	else
		nFace = nRnd * 256 / (2 * math.pi)
		return nFace
	end
end

--霸刀CD转化功能
function CDTransfer(player, CDID_Sou, IsOverDraft_Sou, CDID_Dst, IsOverDraft_Dst)
	--print("------")
	local nLeftFrame_Sou = player.GetCDLeft(CDID_Sou)
	local nLeftFrame_Dst = player.GetCDLeft(CDID_Dst)
	--print("nLeftFrame_Sou" .. nLeftFrame_Sou)
	--print("nLeftFrame_Dst"..nLeftFrame_Dst)
	local nLeftFrame_Dst_modify = 0

	if nLeftFrame_Sou == nLeftFrame_Dst then
		--print(111)
		return
	elseif nLeftFrame_Sou > nLeftFrame_Dst then
		--print(222)
		if IsOverDraft_Dst == 1 then
			local nMaxOverDraftCount, nDraftCount = player.GetOverDraftCoolDown(CDID_Dst)
		end
		player.ResetCD(CDID_Dst)
		if IsOverDraft_Dst == 1 then
			--print(777)
			local nMaxOverDraftCount, nDraftCount = player.GetOverDraftCoolDown(CDID_Dst)
			--处理重烟切体态额外计数BUG：当两者只相差一帧时,不增加透支次数。
			if nLeftFrame_Sou - nLeftFrame_Dst > 1 then
				player.SetOverDraftCoolDown(CDID_Dst, math.min(nDraftCount + 1, nMaxOverDraftCount))
			end
		end
		nLeftFrame_Dst_modify = player.GetCDLeft(CDID_Dst)
		if nLeftFrame_Dst_modify >= nLeftFrame_Sou then
			--print(3333)
			player.ModifyCoolDown(CDID_Dst, nLeftFrame_Sou - nLeftFrame_Dst_modify)	--需要是负值
		else
			if IsOverDraft_Dst == 1 then
				--print(444)
				local nMaxOverDraftCount, nDraftCount = player.GetOverDraftCoolDown(CDID_Dst)
				while(nLeftFrame_Dst_modify < nLeftFrame_Sou)do
					local CDTime = player.GetCDInterval(CDID_Dst)
					player.AddCDTime(CDID_Dst, CDTime)
					player.SetOverDraftCoolDown(CDID_Dst, math.min(nDraftCount + 1, nMaxOverDraftCount))
					nLeftFrame_Dst_modify = nLeftFrame_Dst_modify + CDTime
				end
				player.ModifyCoolDown(CDID_Dst, nLeftFrame_Sou - nLeftFrame_Dst_modify)
			else
				--print(555)
				player.AddCDTime(CDID_Dst, nLeftFrame_Sou - nLeftFrame_Dst_modify)
			end
		end
	else
		--print(666)
		nLeftFrame_Dst_modify = player.GetCDLeft(CDID_Dst)
		player.ModifyCoolDown(CDID_Dst, nLeftFrame_Sou - nLeftFrame_Dst_modify)	--需要是负值
	end
end

--特殊SetPosition 在横云意绷紧状态下若终点超出自身10尺 则此次SetPosition 不执行
function SetSpecialPosition(targetID, nX, nY, nZ)
	local target, ntype
	if not GetPlayer(targetID) then
		target = GetNpc(targetID)
		ntype = TARGET.NPC
	else
		target = GetPlayer(targetID)
		ntype = TARGET.PLAYER
	end

	if not target then
		return
	end
	--NPC直接正常执行
	if ntype == TARGET.NPC then
		target.SetPosition(nX, nY, nZ)
		return
	end

	--对于玩家单位
	if ntype == TARGET.PLAYER then
		--副本组专用不让玩家setposition
		if target.IsHaveBuff(18270, 0) or target.IsHaveBuff(18621, 0) then
			return
		end

		--技能组禁锢
		if target.IsHaveBuff(24450, 0) then
			return
		end
		--不处于横云意状态 直接执行
		if not target.IsHaveBuff(15898, 1) and not target.IsHaveBuff(15897, 1) then
			target.SetPosition(nX, nY, nZ)
			return
		else
			local table_link = target.GetLinkPlayer();
			local nSon = 1
			if table_link[1] ~= 0 then
				local player1 = GetPlayer(table_link[1])
				if player1 then
					local nLp2e = GetDistanceSq(nX, nY, 0, player1.nX, player1.nY, 0)
					if nLp2e > 10 * 64 * 10 * 64 then
						nSon = 0
					end
				end
			end
			if table_link[2] ~= 0 then
				local player2 = GetPlayer(table_link[2])
				if player2 then
					local nLp2e = GetDistanceSq(nX, nY, 0, player2.nX, player2.nY, 0)
					if nLp2e > 10 * 64 * 10 * 64 then
						nSon = 0
					end
				end
			end
			if nSon == 1 then
				target.SetPosition(nX, nY, nZ)
			end
		end
	end

end

