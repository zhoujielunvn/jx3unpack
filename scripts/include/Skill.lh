function SetDelaySubSkillTimeline(skill, tbSubSkillCfg)
	skill.bSubSkillTimeline = true

	local nFrame = 0
	for _, tbSubSkill in ipairs(tbSubSkillCfg) do
		nFrame = nFrame + tbSubSkill.nFrame
		skill.SetDelaySubSkill(nFrame, tbSubSkill.dwSkillID, tbSubSkill.dwSkillLevel)
	end
end

function AddOverDraftCoolDown(player, dwSkilID, dwCooldownID)
    local nMaxOverDraftCount, nDraftCount = player.GetOverDraftCoolDown(dwCooldownID)
    if nMaxOverDraftCount == 0 then
        return
    end

    local dwSkillLevel = player.GetSkillLevel(dwSkilID)
    if dwSkillLevel == 0 then
        return
    end

    local nCooldownInterval = player.GetSkillCDInterval(dwSkilID, dwSkillLevel, dwCooldownID)
    if nCooldownInterval > 0 and nDraftCount < nMaxOverDraftCount then
        player.AddCDTime(dwCooldownID, nCooldownInterval)
        player.SetOverDraftCoolDown(dwCooldownID, nDraftCount + 1)
    end
end

function GetHDKungfuMountID(player)
    local Kungfu = player.GetKungfuMount()
    if not Kungfu then
        return nil
    end

    if Kungfu.nPlatformType ~= SKILL_PLATFORM_TYPE.MOBILE then
        return Kungfu.dwSkillID
    end

    return GetHDKungfuID(Kungfu.dwSkillID)
end

function GetBuffStackNum(character, dwBuffID, dwBuffLevel)
    local buff = character.GetBuff(dwBuffID, dwBuffLevel)
    return buff and buff.nStackNum or 0
end

