--尝试寻找目标点附近固定距离的一个无3D障碍的点
function UseObstacleXY(player, nTargetX, nTargetY, nTargetZ, nHeight, nRadius)
	local scene = player.GetScene()
	if not scene then
		return
	end
	local nResX, nResY, nResZ = player.GetAbsoluteCoordinate()
	if nRadius < 32 then
		return nResX, nResY, nResZ
	end
	local nDir = Random(256)
	for i = 1, 4 do -- 4个方向
		local nPosX, nPosY = CustomFunction.GetOffsetPosByXYFA(nTargetX, nTargetY, 0, nDir, nRadius)
		local nTempX, nTempY, nTempZ, bValid = scene.GetInterceptPoint(nTargetX, nTargetY, nTargetZ, nPosX, nPosY, nTargetZ, nHeight)
		if bValid and (nTempX == 0 and nTempY == 0 and nTempZ == 0) then
			local nGroundX, nGroundY, nGroundZ, bGroundValid = scene.GetInterceptPoint(nPosX, nPosY, nTargetZ, nPosX, nPosY, 0)
			if bGroundValid and (nGroundX ~= 0 or nGroundY ~= 0 or nGroundZ ~= 0) then
				return nGroundX, nGroundY, nGroundZ
			end
		end
		nDir = nDir + 64 -- 加90度
	end
	return nResX, nResY, nResZ
end

--尝试寻找目标点附近固定距离的一个无3D障碍的点
function DzUseObstacleXY(player, nTargetX, nTargetY, nTargetZ, nHeight, nRadius, nCount)
	local scene = player.GetScene()
	if not scene then

		return
	end
	local nDir = player.nFaceDirection
	for i = 1, 4 do -- 4个方向
		for j = 1, nCount do
			local nPosX, nPosY = CustomFunction.GetOffsetPosByXYFA(nTargetX, nTargetY, 0, nDir, nRadius - 64 * nCount)
			local nTempX, nTempY, nTempZ, bValid = scene.GetInterceptPoint(nTargetX, nTargetY, nTargetZ, nPosX, nPosY, nTargetZ, nHeight)
			if bValid and (nTempX == 0 and nTempY == 0 and nTempZ == 0) then
				local nGroundX, nGroundY, nGroundZ, bGroundValid = scene.GetInterceptPoint(nPosX, nPosY, nTargetZ, nPosX, nPosY, 0)
				if bGroundValid and (nGroundX ~= 0 or nGroundY ~= 0 or nGroundZ ~= 0) then
					return nGroundX, nGroundY, nGroundZ
				end
			end
		end
		nDir = nDir + 64 -- 加90度
	end

	local nRadom = Random(100)
	local nFinalDir
	if nRadom <= 50 then
		nFinalDir = player.nFaceDirection + Random(64)
	else
		nFinalDir = player.nFaceDirection - Random(64)
	end
	local nX, nY = CustomFunction.GetOffsetPosByXYFA(nTargetX, nTargetY, 0, nFinalDir, nRadius)
	local nResX, nResY, nResZ = scene.GetInterceptPoint(nX, nY, nTargetZ, nX, nY, 0)
	return nResX, nResY, nResZ
end



