function SkillGetParry(player)
	if player then
		if player.nLevel >= 110 then
			return  (player.nParry / (player.nParry + 4.345 * (player.nLevel * 450 - 45750))) * 100 + player.nParryBaseRate / 100
		else
			return  (player.nParry / (player.nParry + 4.345 * 3750)) * 100 + player.nParryBaseRate / 100
		end
	else
		return 0
	end
end

