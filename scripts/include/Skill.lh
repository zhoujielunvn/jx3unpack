if IsClient() then
	-- 清理客户端角色的移动状态，用于玩家通用解控技能
	function ClearCharacterMoveState(character)
		if character.nMoveState == MOVE_STATE.ON_KNOCKED_OFF or
			character.nMoveState == MOVE_STATE.ON_PULL or
			character.nMoveState == MOVE_STATE.ON_KNOCKED_DOWN or
			character.nMoveState == MOVE_STATE.ON_KNOCKED_BACK or
			character.nMoveState == MOVE_STATE.ON_REPULSED or
			character.nMoveState == MOVE_STATE.ON_SKILL_MOVE_DEATH or
			character.nMoveState == MOVE_STATE.ON_SKILL_MOVE_DST or
			character.nMoveState == MOVE_STATE.ON_SKILL_MOVE_SRC or
			character.nMoveState == MOVE_STATE.ON_SKILL_MOVE_TAIL
			then
			-- 服务器端的移动状态变更同步到客户端有延迟
			-- 此处在客户端先清理角色状态并同步到表现，避免角色动作逻辑错误
			character.Stop()
			character.PostStateToRepresent()
		end
	end
end
