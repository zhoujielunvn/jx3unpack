function HongChenXiaYing_AddAssistedLiXingYunPower(player, nPower)
	local lxyjj = player.GetNpcAssistedStagePoint(14)  --李星云心法和武学境界三
	--检测李星云是否在场
	local bLXYIn = false
	local npclxy
	local tHero = player.GetSummonedList()
	if not tHero then
		return
	end
	for k, v in pairs(tHero) do
		if v.dwAssistedID == 14 then
			bLXYIn = true
			npclxy = GetNpc(v.dwNpcID)
			break
		end
	end
	if not npclxy then
		return
	end
	--在场则开始加战势值
	local bufflxy = player.GetBuff(25599, 2)
	if not bufflxy then
		if lxyjj < 15000 then
			player.AddAssistedPower(14, nPower)
			npclxy.AddBuff(player.dwID, player.nLevel,25599,2,1,0,nPower/50)
		end
		if  lxyjj >= 15000  then
			player.AddAssistedPower(14, nPower * 1.5)
			npclxy.AddBuff(player.dwID, player.nLevel,25599,2,1,0,nPower * 1.5/50)
		end
	else
		if bufflxy.nStackNum < 17 and lxyjj < 15000  then
			player.AddAssistedPower(14, nPower)
			npclxy.AddBuff(player.dwID, player.nLevel,25599,2,1,0,nPower/50)
		end
		if bufflxy.nStackNum < 25 and lxyjj >= 15000  then
			player.AddAssistedPower(14, nPower * 1.5)
			npclxy.AddBuff(player.dwID, player.nLevel,25599,2,1,0,nPower * 1.5/50)
		end
	end
end


function WanHuaRandom(n)
	local set = {}
	for i = 0, 2 ^ n - 1 do
		local nOnes = 0
		for j = 0, n - 1 do
			if GetValueByBits(i, j, 1) == 0 then
				nOnes = nOnes + 1
			end
		end
		if nOnes == n / 2 then
			table.insert(set, i)
		end
	end
	return set
end
