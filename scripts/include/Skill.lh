function wanhuaguNewPower(player, nAdd, bOnlyFresh, bShuiyue, bNotFight) --增删能量, bOnlyFresh是否仅刷新，bShuiyue是否水月期间，bNotFight是否处于非战斗状态（非战斗状态下能量不受奇穴加减影响）
	if not bOnlyFresh then --如果只刷新，就处理一下nAdd
		if nAdd == 0 then
			return
		end
		if player.GetSkillLevel(32484) == 1 and not bNotFight then --放歌加能量额外+2，非战斗状态下不加
			nAdd = nAdd + 2
		end
		--新雪中行
		--if not bNotFight and nAdd < 0 and player.GetSkillLevel(32477) == 1 then
			--if not player.GetBuff(24205, 1) or player.GetBuff(24205, 1).nStackNum < 3 then
			--player.AddBuff(player.dwID, player.nLevel, 24205, 1)
			--end
		--end
	end
	--local buffStack = 0
	--local buff = player.GetBuff(24156, 1)
	--if buff then
	--buffStack = buff.nStackNum
	--end
	--if buffStack + nAdd < 0 then --无效扣能
	--return
	--end

	if nAdd > 0 then --脱战重置墨意计数
		local buff_tzmy = player.GetBuff(24277, 0)
		if buff_tzmy then
			buff_tzmy.nCustomValue = 0
		end
	end
	player.nCurrentRage = player.nCurrentRage + nAdd
	if player.nCurrentRage > player.nMaxRage then
		player.nCurrentRage = player.nMaxRage
	end
	--buff掩码16646
	player.DelMultiGroupBuffByID(412)
	player.DelMultiGroupBuffByID(6266) --先把原本的两个删掉
	--player.DelMultiGroupBuffByID(24156) --表现用，UI来了就注掉
	--if player.nCurrentRage > 0 then
	--player.AddBuff(player.dwID, player.nLevel, 24156, 1, 1, 0, player.nCurrentRage)
	--end
	if nAdd < 0 and not player.IsHaveBuff(24245, 0) then --流离专属，消耗后把全瞬发删除
		player.DelBuff(24538, 1)
	end

	--if player.nCurrentRage >= 10 then --乱洒
		--player.AddBuff(player.dwID, player.nLevel, 26280, 1)
	--else
		--player.DelBuff(26280, 1)
	--end
	if player.nCurrentRage >= 40 then--新的流迟彼针
		player.AddBuff(player.dwID, player.nLevel, 27091, 1)

	else
		player.DelBuff(27091, 1)
	end

	local nSFBuffStack = math.floor(player.nCurrentRage / 20) --20能量1瞬发
	if nSFBuffStack > 0 then
		if player.IsHaveBuff(24245, 0) or player.IsHaveBuff(24538, 0) then
			bShuiyue = true
		end
		if bShuiyue then
			player.AddBuff(player.dwID, player.nLevel, 412, 1, 1, 0, nSFBuffStack)
			local Is_pre, S_ID, S_lev, L_time = player.GetSkillOTActionState()
			if Is_pre ~= CHARACTER_OTACTION_TYPE.ACTION_IDLE then
				player.AddBuff(player.dwID, player.nLevel, 8597, 1) --保护buff
				--player.AddBuff(player.dwID, player.nLevel, 8597, 1) --保护buff
			end
		else
			player.AddBuff(player.dwID, player.nLevel, 6266, 1, 1, 0, nSFBuffStack)
			local Is_pre, S_ID, S_lev, L_time = player.GetSkillOTActionState()
			if Is_pre ~= CHARACTER_OTACTION_TYPE.ACTION_IDLE and (S_ID == 179 or S_ID == 142 or S_ID == 140) then --三针读条时能量加了触发，加上保护（）
				player.AddBuff(player.dwID, player.nLevel, 8597, 1) --保护buff
				--player.AddBuff(player.dwID, player.nLevel, 8597, 1) --保护buff 不知道为什么要加2层，抄放歌的应该没事
			end
		end
		RemoteCallToClient(player.dwID, "ChangeSkillSurfaceNum", 142, nSFBuffStack)
		RemoteCallToClient(player.dwID, "ChangeSkillSurfaceNum", 179, nSFBuffStack)
	else
		RemoteCallToClient(player.dwID, "ChangeSkillSurfaceNum", 142, nil)
		RemoteCallToClient(player.dwID, "ChangeSkillSurfaceNum", 179, nil)
	end
end

