--目标身上所有伤害吸收盾减少百分比 参数一目标玩家的dwID(只支持玩家) 参数二需要扣除的百分比
--若目标身上有多个吸收盾，每个吸收盾都扣除对应比例
function CostAllDamageAbsorbShieldByPercent(dwSkillSrcID, nLevel, dwCharacterID, nPercent)
	local player = GetPlayer(dwCharacterID)
	if not player then
		return
	end

	local t_Absorb = player.GetAbsorbInfo()
	if #t_Absorb > 0 then
		for i, v_AbsorbMessage in pairs(t_Absorb) do
			for j, k_AbsorbType in pairs(t_AbsorbDamageShieldType) do
				if v_AbsorbMessage.nAbsorbType == k_AbsorbType then
					player.CostAbsorbShieldValue(dwSkillSrcID, 30428, nLevel, v_AbsorbMessage.dwBuffID, player.GetAbsorbShieldLeftValue(v_AbsorbMessage.dwBuffID) * nPercent, true)
					break;
				end
			end
		end
	end
end

--目标身上所有伤害吸收盾减少指定数值 参数一目标玩家的dwID(只支持玩家) 参数二需要扣除的数值
--若目标身上有多个吸收盾，依据吸收盾的顺序依次消耗
function CostAllDamageAbsorbShieldByValue(dwSkillSrcID, nLevel, dwCharacterID, nValue)
	local player = GetPlayer(dwCharacterID)
	if not player then
		return
	end
	local t_Absorb = player.GetAbsorbInfo()
	local nLeftValue = nValue
	if #t_Absorb > 0 then
		for i, v_AbsorbMessage in pairs(t_Absorb) do
			for j, k_AbsorbType in pairs(t_AbsorbDamageShieldType) do
				if v_AbsorbMessage.nAbsorbType == k_AbsorbType then
					local nCostValue = math.min(v_AbsorbMessage.nAbsorbValue, nLeftValue)
					nLeftValue = nLeftValue - nCostValue
					player.CostAbsorbShieldValue(dwSkillSrcID, 30428, nLevel, v_AbsorbMessage.dwBuffID, nCostValue, true)
					break;
				end
			end
			if nLeftValue <= 0 then
				break;
			end
		end
	end
	if nLeftValue > 0 then
		player.RechargeableAbsorbDamage(dwSkillSrcID, 30428, nLevel, nLeftValue, true)
	end
end

--目标身上所有伤害吸收盾减少指定数值 参数一目标玩家的dwID(只支持玩家) 参数二需要扣除的百分比
--若目标身上有多个吸收盾，依据吸收盾的顺序依次消耗百分比，nPer值小数点1.0=100%,技能显示：护盾衰减
