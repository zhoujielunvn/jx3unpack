-- 检查是否满足某种地皮大小的条件 1：1000平(没有)  2：2000平  3：4000平
-- false表示不满足条件，0表示钱不够，1表示满足条件且钱够
function CheckHomeCondition_462(player, nLandTpye) -- 1：1000平  2：2000平  3：4000平
	local bFlag = false
	if not tLandCondition_462[nLandTpye] then
		return bFlag
	end

	-- 房屋等级成就，九寨沟是必要条件
	if tLandCondition_462[nLandTpye].nAchievementID and not(player.IsAchievementAcquired(tLandCondition_462[nLandTpye].nAchievementID)) then
		return bFlag
	end

	--其他资格的成就,满足其中一条即可
	if tLandCondition_462[nLandTpye].nAchievementID1 and player.IsAchievementAcquired(tLandCondition_462[nLandTpye].nAchievementID1) then
		bFlag = true
	end
	if tLandCondition_462[nLandTpye].nAchievementID2 and player.IsAchievementAcquired(tLandCondition_462[nLandTpye].nAchievementID2) then
		bFlag = true
	end

	if bFlag then
		if player.GetMoney().nGold < tLandCondition_462[nLandTpye].nMoney then
			return 0
		else
			return 1
		end
	else
		return false
	end
end

--原始点X，原始点Y,原始点朝向，半径，偏移角度（0表示正前方，180表示正后方）
function GetPositionByAngleAndRadius(nX, nY, nFaceDirection, nRadius, nAngle)
	local nAngle0 = math.floor((360 * nFaceDirection) / 256)     --游戏中256的朝向表示360度； 原始朝向对应的角度
	local nAngle1 = nAngle0 + nAngle
	if nAngle1 > 360 then
		nAngle1 = nAngle1 - 360
	end
	local nX1 = math.floor(nX + math.cos(math.rad(nAngle1)) * nRadius)
	local nY1 = math.floor(nY + math.sin(math.rad(nAngle1)) * nRadius)
	nAngle1 = math.floor(256 * nAngle1 / 360)
	--print("nAngle:::" .. nAngle .. "   nAngle1:::" .. nAngle1)
	--print("nX:::" .. nX .. "     nY:::" .. nY)
	--print("nX1:::" .. nX1 .. "     nY1:::" .. nY1)
	return nX1, nY1, nAngle1
end

--检查经营模式条件
--nBusnessType经营类型
--nTaskType： "HOMELAND_TASK_TYPE.BUILDING","HOMELAND_TASK_TYPE.INSTALL","HOMELAND_TASK_TYPE.UNINSTALL",
function CheckHomeLandBusness(dwMapID, nCopyIndex, nLandIndex, nBusnessType, nTaskType)
	if not IsHomeLandMap(dwMapID) then
		return
	end
	if not tBusnessHome[nBusnessType] then
		return
	end

	--棋牌室检查
	if nBusnessType == GDENUM_HOMELAND_MARKET_TYPE.CARD then			
		local nCommunity = HomeLandMgr.GetCommunity(dwMapID, nCopyIndex).nIndex--小区几号线	
		local nNum = HomeLandMgr.GetCategoryCount(dwMapID, nCopyIndex, nLandIndex, tBusnessHome[nBusnessType].category)
		if nNum >= tBusnessHome[nBusnessType].num then	--检查二级类数量
			local nKey = dwMapID * 1000000 + nCommunity * 100 + nLandIndex	--landindex不超过100,分线id不超过10000
			local nRankTime = GetCurrentTime()
			UpdateCustomRankList(264, nKey, nRankTime, {scene = dwMapID, copy = nCopyIndex, land = nLandIndex, community = nCommunity})			
			SetLandBusnessType(dwMapID, nCopyIndex, nLandIndex, GDENUM_HOMELAND_MARKET_TYPE.CARD, true)			
			if nTaskType == HOMELAND_TASK_TYPE.BUILDING then	--主人正在建造，那么取到主人信息，提示
				local nOwnerID = HomeLandMgr.GetLandOwner(dwMapID, nCopyIndex, nLandIndex)
				local player = GetPlayer(nOwnerID)
				if player then
					player.SendSystemMessage(string.format(GetEditorString(20, 9071), tBusnessHome[nBusnessType].num))						
				end
			end
		else	--数量不够就撤图标
			SetLandBusnessType(dwMapID, nCopyIndex, nLandIndex, GDENUM_HOMELAND_MARKET_TYPE.CARD, false)	
		end
	end

	--饭馆检查
	if nBusnessType == GDENUM_HOMELAND_MARKET_TYPE.DINE then			
		local nCommunity = HomeLandMgr.GetCommunity(dwMapID, nCopyIndex).nIndex--小区几号线	
		local nNum = HomeLandMgr.GetCategoryCount(dwMapID, nCopyIndex, nLandIndex, tBusnessHome[nBusnessType].category)
		if nNum >= tBusnessHome[nBusnessType].num then	--检查二级类数量
			local nOwnerID = HomeLandMgr.GetLandOwner(dwMapID, nCopyIndex, nLandIndex)
			local player = GetPlayer(nOwnerID)
			if player then
				if CheckHomeLandBusnessNum(dwMapID, nCopyIndex, nLandIndex, nBusnessType) then
					player.SendSystemMessage(string.format(GetEditorString(20, 9081), tBusnessHome[nBusnessType].num))
					SetLandBusnessType(dwMapID, nCopyIndex, nLandIndex, GDENUM_HOMELAND_MARKET_TYPE.DINE, true)
				else					
					player.SendSystemMessage(string.format(GetEditorString(20, 9082), tBusnessHome[nBusnessType].num))					
				end
			end
		else	--数量不够就撤图标
			SetLandBusnessType(dwMapID, nCopyIndex, nLandIndex, GDENUM_HOMELAND_MARKET_TYPE.DINE, false)	
		end
	end

	--商人六爷检查
	if nBusnessType == GDENUM_HOMELAND_MARKET_TYPE.SELL then
		return IfLandBusnessTypeOpen(dwMapID, nCopyIndex, nLandIndex, GDENUM_HOMELAND_MARKET_TYPE.SELL)
	end	
end

--检查维持经营类型所需的商品数量是否足够（如饭馆的食物）
function CheckHomeLandBusnessNum(dwMapID, nCopyIndex, nLandIndex, nBusnessType)
	if not IsHomeLandMap(dwMapID) then
		return
	end
	if not tBusnessHome[nBusnessType] then
		return
	end

	--饭馆食物数量检查检查
	if nBusnessType == GDENUM_HOMELAND_MARKET_TYPE.DINE then
		local nDeskNum = HomeLandMgr.GetCategoryCount(dwMapID, nCopyIndex, nLandIndex, tBusnessHome[nBusnessType].category)		
		if nDeskNum == 0 then
			SetLandBusnessType(dwMapID, nCopyIndex, nLandIndex, GDENUM_HOMELAND_MARKET_TYPE.DINE, false)
			return false
		else	--检查食物数量
			local nLeftNum = 0			
			for i = 1, nDeskNum do
				local tFurnitureInfo = HomeLandMgr.GetLOByCategory(dwMapID, nCopyIndex, nLandIndex, tBusnessHome[nBusnessType].category, i)
				local tDwFour = HomeLandMgr.GetSDFourDwordScript(dwMapID, nCopyIndex, nLandIndex, tFurnitureInfo.nObjectID, 1)--读数据
				if tDwFour then
					local nFoodNum = HomeLandMgr.GetDWORDValueByuint8(tDwFour[2], 0)--食物数量，均瑶存在这里，详见摆宴.lua
					nLeftNum = nLeftNum + nFoodNum	
				end							
			end
			--print("nLeftNum", nLeftNum)			
			if nLeftNum > 0 then
				SetLandBusnessType(dwMapID, nCopyIndex, nLandIndex, GDENUM_HOMELAND_MARKET_TYPE.DINE, true)
				return true				
			else
				SetLandBusnessType(dwMapID, nCopyIndex, nLandIndex, GDENUM_HOMELAND_MARKET_TYPE.DINE, false)
				return false	
			end
		end
	end
end

