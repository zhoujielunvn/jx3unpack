function UpdateAchieBuff(player, nBuffID)
	-- 每次睡觉 时间存到第二天七点+24小时
	--如果两次睡觉时间过七点，堆叠层数+1
	--如果没过七点就更新剩余调数。
	if player.IsAchievementAcquired(tBuffAchieve[nBuffID]) then
		return
	end
	local buffCount = player.GetBuff(nBuffID, 1)
	if not buffCount then
		player.AddBuff(0, 99, nBuffID, 1, math.floor(OnGetNextSevenPassTime() / 60) + 24 * 60)
		buffCount = player.GetBuff(nBuffID, 1)
		buffCount.nCustomValue = GetCurrentTime()
		return
	end
	local nOldTime = buffCount.nCustomValue
	local nNowTime = GetCurrentTime()
	if OnCheckPassSevenTimeRequest(nOldTime, nNowTime) then
		player.AddBuff(0, 99, nBuffID, 1, math.floor(OnGetNextSevenPassTime() / 60 + 24 * 60))
		buffCount.nCustomValue = GetCurrentTime()
		if buffCount.nStackNum == 7 then
			player.AcquireAchievement(tBuffAchieve[nBuffID])
		end
	end
end

