--增加本小区活跃值
function AddLandnActiveness(player, nType, nCount)
	if not player then
		return
	end
	local scene = player.GetScene()
	if not scene then
		return
	end
	local dwMapID = scene.dwMapID
	if not IsHomeMap(dwMapID) then
		return
	end
	if not nCount then
		nCount = 1
	end

	local nNum = tLandActivity[nType].nAddValue
	local nTotalAddValue = nNum * nCount
	local nLandID = HomeLandMgr.IsCommunityMember(player.dwID)
	local bIsPrivateHomeMap = IsPrivateHomeland(dwMapID)
	if nLandID == 0 or bIsPrivateHomeMap then --不是自己的小区或者是私宅，则统一挂上活跃度转移buff
		if nTotalAddValue > 0 then
			AddActivityBuff(player, nTotalAddValue)
		end	
		return
	end
	-----------------------以下全为社区家园--------------------------
	if bIsPrivateHomeMap then
		return
	end
	
	if nType == 2 then --如果是送信日常，且做任务的人已开同居
		local tCurHomeMemberInfo = HomeLandMgr.GetLandAlliedBaseInfo(scene.dwMapID, scene.nCopyIndex, nLandID)	--同住人员的名单信息
		local nCurFriendNum = tCurHomeMemberInfo.Count	--当前合租人数，不含主人
		if nCurFriendNum > 0 then
			nNum = nNum + 10
			player.SendSystemMessage(GetEditorString(17, 6599))
		end
	end
	AddActivityInScene(player, nNum * nCount)
end

