--设置家园的经营类型,tMARKET_TYPE示例GDENUM_HOMELAND_MARKET_TYPE.CARD，nOpen=0/1
function SetLandBusnessType(dwMapID, nCopyIndex, nLandIndex, nBusnessType, bOpen)
	if not dwMapID or not nCopyIndex or not nLandIndex then
		return
	end
	if dwMapID == 0 or nCopyIndex == 0 or nLandIndex == 0 then
		return
	end	
	if not tBusnessHome[nBusnessType] then
		return
	end
	HomeLandMgr.SetLandMarketType(dwMapID, nCopyIndex, nLandIndex, nBusnessType, bOpen)
end

function IfLandBusnessTypeOpen(dwMapID, nCopyIndex, nLandIndex, nBusnessType)
	if not IsHomeMap(dwMapID) then
		return false
	end
	if not tBusnessHome[nBusnessType] then
		return false
	end
	return HomeLandMgr.GetLandMarketType(dwMapID, nCopyIndex, nLandIndex, nBusnessType)	
end

