-- 检查是否满足某种地皮大小的条件 1：1000平  2：2000平  3：4000平
-- false表示不满足条件，0表示钱不够，1表示满足条件且钱够
function CheckHomeCondition_455(player, nLandTpye) -- 1：1000平  2：2000平  3：4000平
	local bFlag = false
	if not tLandCondition_455[nLandTpye] then
		return false
	end

	-- 角色等级
	if player.nLevel < tLandCondition_455[nLandTpye].nLevel then
		bFlag = false
	end

	-- 成就点数
	if not bFlag and tLandCondition_455[nLandTpye].nAchievementScore and player.GetAchievementRecord() >= tLandCondition_455[nLandTpye].nAchievementScore then
		bFlag = true
	end

	-- 房屋等级成就
	if not bFlag and tLandCondition_455[nLandTpye].nAchievementID and player.IsAchievementAcquired(tLandCondition_455[nLandTpye].nAchievementID) then
		bFlag = true
	end

	-- 商城道具成就
	if not bFlag and tLandCondition_455[nLandTpye].nAchievementID1  and player.IsAchievementAcquired(tLandCondition_455[nLandTpye].nAchievementID1) then
		bFlag = true
	end

	-- 装分
	if not bFlag then
		local nEquip = player.GetTotalEquipScore()
		if nEquip and nEquip >= tLandCondition_455[nLandTpye].nEquipmentScore then
			bFlag = true
		else
			bFlag = false
		end
	end

	if bFlag then
		if player.GetMoney().nGold < tLandCondition_455[nLandTpye].nMoney then
			return 0
		else
			return 1
		end
	else
		return false
	end
end

