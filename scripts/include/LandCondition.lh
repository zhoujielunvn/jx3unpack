--检查等级是否满足（未配置返回-1，符合要求返回TRUE，不符合要求返回FALSE）
function CheckHomeConditionLevel(player, tLandCondition)
	if not tLandCondition.nLevel then
		return - 1
	end
	if player.nLevel >= tLandCondition.nLevel then
		return true
	else
		return false
	end
end
--检查成就总点数是否满足（未配置返回-1，符合要求返回TRUE，不符合要求返回FALSE）
function CheckHomeConditionAchiSore(player, tLandCondition)
	if not tLandCondition.nAchievementScore then
		return - 1
	end
	if player.GetAchievementRecord() >= tLandCondition.nAchievementScore then
		return true
	else
		return false
	end
end
