				table.remove(tRankList)
			end
		else
			tRankList = {{nMapCopyIndex, nCenterID}}
		end
		UpdateCustomRankList(tMapLinkRankList[scene.dwMapID][1], nSeedId, 1, tRankList)

		local szServerName = tServerCenterName[nCenterID]
		local szMapName = tMapLinkRankList[scene.dwMapID][2]
		local szBranch = GetEditorString(1, 9489) .. nMapCopyIndex ..GetEditorString(18, 6186)
		local szFlower = ""
		if nType == 1 then
			szFlower = string.format("%s(%s)", tSeed[nSeedId][12], tSeed[nSeedId][13])
		elseif nType == 2 then
			szFlower = tSeed[nSeedId][12]
		end
		-- 家园花价数据同步到推栏app
		RemoteCallToChatServer("On_HomeLand_SendFlowerPrice", szServerName, szMapName, szBranch, szFlower)
	end
end

