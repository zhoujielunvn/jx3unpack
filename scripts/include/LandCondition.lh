local tHomeGuideQuest = {21701, 21702, 21656, 21611, 21612, 21778, 21703, 21779, 21780, 21698, 21781, 21746, 21755, 21747, 21783, 21784, 21754}
function JumpTeachQuest(player, nType)
	for k, v in pairs(tHomeGuideQuest) do
		local nQuestPhase = player.GetQuestPhase(v)
		if nQuestPhase ~= 3 then
			if nQuestPhase == 0 then
				player.AcceptQuest(1, 1, v, 1, 0)
			end
			player.ForceFinishQuest(v)
		end
	end
	if nType == 1 then	--共居
		RemoteCallToClient(player.dwID, "OnOutputWarningMessage", "MSG_WARNING_YELLOW", GetEditorString(22, 6764), 5)
	elseif nType == 2 then		--入住枫叶泊
		RemoteCallToClient(player.dwID, "OnOutputWarningMessage", "MSG_WARNING_YELLOW", GetEditorString(23, 6631), 5)
	else		--无理由跳过
		RemoteCallToClient(player.dwID, "OnOutputWarningMessage", "MSG_WARNING_YELLOW", GetEditorString(23, 9143), 5)
	end	
end