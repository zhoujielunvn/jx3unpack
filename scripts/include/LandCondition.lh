--本小区添加活跃度到场景和地皮
function AddActivityInScene(player, nAddActivitness)
	local scene = player.GetScene()
	if not scene then
		return
	end

	local dwMapID    = scene.dwMapID
	local nCopyIndex = scene.nCopyIndex
	local nLandID    = HomeLandMgr.IsCommunityMember(player.dwID)
	if nLandID == 0 or IsPrivateHomeland(dwMapID) then
		return
	end

	local nLandActivity = scene.GetCustomUnsigned2(SCENE_CUSTOM_VALUE_NAME.GUANGLINGYI.LANDACTIVITY + (nLandID - 1) * 2)
	scene.SetCustomUnsigned2(SCENE_CUSTOM_VALUE_NAME.GUANGLINGYI.LANDACTIVITY + (nLandID - 1) * 2, nLandActivity + (nAddActivitness))
	if nAddActivitness > 1 then
		scene.SaveMap()	
	end

	local tCommunity = HomeLandMgr.GetCommunity(dwMapID, nCopyIndex)
	if tCommunity.nActiveness >= 1073741824  then --2的30次方
		return
	end
	if nAddActivitness > 0 then
		player.SendSystemMessage(GetEditorString(17, 465)..nAddActivitness..GetEditorString(1, 734))
		RemoteCallToClient(player.dwID, "OnOutputWarningMessage", "MSG_NOTICE_GREEN", GetEditorString(17, 465)..nAddActivitness..GetEditorString(1, 734), 5)
	end
	if tCommunity.bCommunityReady then
		HomeLandMgr.SetCommunityActiveness(dwMapID, nCopyIndex, tCommunity.nActiveness + nAddActivitness)
	end
end

